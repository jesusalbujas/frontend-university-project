{
  "server": {
    "host": "api.adempiere.io",
    "port": 9527,
    "publicPath": "/base-public-path/",
    "vueVersion": "develop-1.0.0"
  },
  "adempiere": {
    "api": {
      "url": "http://api.adempiere.io/api/",
      "timeout": 99999999
    },
    "images": {
      "url": "http://api.adempiere.io/api/resources"
    },
    "resource": {
      "url": "http://api.adempiere.io/api/resources"
    }
  },
  "repository": {
    "url": "https://api.github.com",
    "releaseNo": "releaseNoForDocumentation"
  },
  "taskManager": {
    "api": {
      "url": "http://api.adempiere.io:8080/v1",
      "endPoints": {
        "jobs": "/jobs",
        "runJob": "/jobs",
        "restoreJob": "/restore"
      }
    }
  },
  "documentation": {
    "api": {
      "url": "https://api.github.com/repos/adempiere/"
    }
  },
  "betaFunctionality": {
    "window": false,
    "process": false,
    "report": false,
    "smartBrowser": false,
    "form": false
  },
  "express":{
    "receipt": "/express-receipt",
    "movement": "/express-movement",
    "shipment": "/express-shipment"
  },
  "issuesManagement": {
    "endpoint": "/issue-management"
  },
  "generalGedger": {
    "accounts": "/general-ledger/accounts",
    "organizations": "/general-ledger/organizations",
    "rePost": "/general-ledger/re-post"
  },
  "match": {
    "endpoint": "/form/addons/match"
  },
  "pointOfSales": {
    "endpoint": "/point-of-sales"
  },
  "priceChecking": {
    "endpoint": "/point-of-sales"
  },
  "payrollActionNotice": {
    "endpoint": "/payroll-action-notice"
  },
  "session": {
    "timeout": 300000
  },
  "vPayPrint": {
    "endpoint": "/payment-print-export"
  },
  "VBankStatementMatch": {
    "endpoint": "/bank-statement-match"
  },
  "weight": {
    "endpoint": "/form/addons/weight"
  },
  "noticeManagement": {
    "endpoint": "/notice-management"
  },
  "trialBalanceDrillable": {
    "endpoint": "/trial-balance-drillable"
  },
  "theme": "src/themes/default"
}
